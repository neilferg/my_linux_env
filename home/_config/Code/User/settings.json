{
    "[cpp]": {
        "editor.autoIndent": "none"
    },
    "files.exclude": {
        "Binaries/": true,
        "Documents/": true,
        "External": true,
        "**/.settings": true,
        "**/*.tar": true,
        "**/*.tar.*": true,
        "doxygen": true,
        ".kate*": true,
        "gtags*": true,
        "GTAGS": true,
        "GPATH": true,
        "GRTAGS": true,
        ".ccache":true,
        "Coyote/LTIB/**/rpm":true
    },
    "files.associations": {
        "*.in": "makefile",
        "Makefile.*": "makefile",
        "*.mk": "makefile"
    },
    "workbench.startupEditor": "newUntitledFile",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "gitlens.keymap": "chorded",
    "workbench.iconTheme": "vscode-icons",
    "diffEditor.ignoreTrimWhitespace": true,
    "vsicons.dontShowNewVersionMessage": true,
    "C_Cpp.intelliSenseEngine": "Default",
    "clang.cxxflags": [
        "-DOS_QT_USED",
        "-I/usr/include/qt4/QtCore",
        "-I/usr/include/qt4/QtGui",
        "-I/usr/include/qt4/QtNetwork",
        "-I/usr/include/qt4/Qt",
        "-I/usr/include/qt4/",
        "-I/usr/local/Trolltech/Qt-4.8.6/include",
        "-I/usr/local/Trolltech/Qt-4.8.6/include/QtCore",
        "-I/usr/local/Trolltech/Qt-4.8.6/include/QtGui",
        "-I/usr/local/Trolltech/Qt-4.8.6/include/Qt",
        "-I/usr/local/Trolltech/Qt-4.8.6/include/QtNetwork",
        "-I${workspaceRoot}/RR-Workspace/RR/build/Shared",
        "-I${workspaceRoot}/RR-Workspace/RR/build/Shared/MeasMonitor/Include",
        "-I${workspaceRoot}/Shared",
        "-I${workspaceRoot}/Shared/Serialization/Include",
        "-I${workspaceRoot}/Coyote/Shared/Phy/Include",
        "-I${workspaceRoot}/Coyote/Shared/Gianfar/Include",
        "-I${workspaceRoot}/Coyote/VitesseApi/Extended",
        "-I${workspaceRoot}/Coyote/VitesseApi/Main/include",
        "-I${workspaceRoot}/RR-Workspace/RR/build/NiDevice/Driver/Plugins/Vxi11/LanguageInterface/CTcpIpDevice",
        "-I${workspaceRoot}/RR-Workspace/RR/build/NiDevice/Driver/Plugins/Vxi11/LanguageInterface/Shared",
        "-I${workspaceRoot}/RR-Workspace/RR/build/NiDevice/Driver/Plugins/Vxi11/Shared",
        "-I${workspaceRoot}/RR-Workspace/RR/build/NiDevice/Driver/Engine/LanguageInterface",
        "-I${workspaceRoot}/RR-Workspace/RR/build/NiDevice/Driver/Engine/Shared",
        "-I${workspaceRoot}/RR-Workspace/RR/build/apps/RRMainX",
        "-I.",
        "-std=c++11"
    ],
    "clang.completion.maxBuffer" : 1000,
    "clang.completion.enable": false,
    "clang.diagnostic.maxBuffer" : 1000000,
    "clang.diagnostic.delay": 1000,
    "gitlens.historyExplorer.enabled": true,
    "editor.formatOnType": true,
    "editor.mouseWheelZoom": true,
    "editor.roundedSelection": true,
    "editor.rulers": [
        80,
        120
    ],
    "window.enableMenuBarMnemonics": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "debug.inlineValues": true,
    "debug.openExplorerOnEnd": true,
    "debug.toolBarLocation": "docked",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "Courier",
    "terminal.integrated.scrollback": 5000,
    "C_Cpp.workspaceParsingPriority": "high",
    "C_Cpp.clang_format_sortIncludes": false,
    "window.zoomLevel": 0,
    "editor.quickSuggestionsDelay": 500,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "update.mode": "manual",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "npm.fetchOnlinePackageInfo": false,
    "settingsSync.keybindingsPerPlatform": false,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "matlab.mlintpath": "/bin/true",
    "workbench.colorTheme": "Eclipse Color Theme",
    "matlab.mlintpath": "/bin/true",
    "python.formatting.provider": "none"
}
